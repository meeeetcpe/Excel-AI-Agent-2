{
  "name": "office-addin-manifest",
  "version": "2.1.0",
  "description": "Read and modify Office Add-in manifest files.",
  "main": "./lib/main.js",
  "scripts": {
    "build": "rimraf lib && concurrently \"tsc -p tsconfig.json\"",
    "cli": "node lib/cli.js",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "watch": "rimraf lib && concurrently \"tsc -p tsconfig.json -w\""
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-manifest": "./cli.js"
  },
  "keywords": [
    "Office",
    "Office Add-in"
  ],
  "dependencies": {
    "@microsoft/app-manifest": "^1.0.0-rc.0",
    "adm-zip": "0.5.16",
    "chalk": "^2.4.2",
    "commander": "^13.0.0",
    "fs-extra": "^7.0.1",
    "node-fetch": "^2.6.1",
    "office-addin-usage-data": "^2.0.4",
    "uuid": "^8.3.2",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^10.0.6",
    "@types/node": "^14.17.2",
    "@types/node-fetch": "^2.5.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^9.4.4",
    "@types/xml2js": "^0.4.5",
    "concurrently": "^9.0.0",
    "copy-dir": "^0.4.0",
    "mocha": "^11.0.1",
    "office-addin-lint": "^3.0.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "validator": "^13.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  },
  "prettier": "office-addin-prettier-config",
